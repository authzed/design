{
  "version": 2,
  "builds": [
    {
      "src": "sandworm/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "basin/package.json",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/basin/(.*)",
      "dest": "/basin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/sandworm/$1"
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  }
}
